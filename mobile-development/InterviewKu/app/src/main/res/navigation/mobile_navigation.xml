<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.capstone.interviewku.ui.fragments.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_interview"
        android:name="com.capstone.interviewku.ui.fragments.interview.InterviewFragment"
        android:label="@string/title_TextToSpeach"
        tools:layout="@layout/fragment_interview" />

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.capstone.interviewku.ui.fragments.account.AccountFragment"
        android:label="@string/title_account"
        tools:layout="@layout/fragment_account" />
</navigation>